{
  "name": "wpseed/widgetizer",
  "authors": [
    {
      "name": "Zorca Orcinus",
      "email": "vs@zorca.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "vendor/wordpress-plugin/{$name}": ["type:wordpress-plugin"]
    }
  },
  "require": {
    "psr/container": "^1.0",
    "latte/latte": "^2.4",
    "symfony/finder": "^3.4",
    "symfony/yaml": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "dealerdirect/phpcodesniffer-composer-installer": "0.5.0",
    "wp-coding-standards/wpcs": "^2.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "php-coveralls/php-coveralls": "^2.1",
    "wp-cli/wp-cli": "dev-master"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "includes",
      "vendor/wp-cli"
    ]
  }
}
