{
  "name": "wpseed/widgetizer",
  "authors": [
    {
      "name": "Zorca Orcinus",
      "email": "vs@zorca.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "vendor/wordpress-plugin/{$name}": ["type:wordpress-plugin"]
    }
  },
  "require": {
    "latte/latte": "^2.4",
    "symfony/finder": "^3.4",
    "nette/neon": "^2.4",
    "nette/utils": "^2.5",
    "symfony/filesystem": "^3.4",
    "nette/php-generator": "^2.6",
    "symfony/http-foundation": "^3.4",
    "pimple/pimple": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "nette/tester": "^2.0",
    "dealerdirect/phpcodesniffer-composer-installer": "0.5.0",
    "wp-coding-standards/wpcs": "^2.0",
    "phpcompatibility/phpcompatibility-wp": "*",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Wpseed\\Widgetizer\\": "includes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wpseed\\Widgetizer\\": "includes"
    }
  }
}
