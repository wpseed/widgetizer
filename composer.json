{
  "name": "wpseed/widgetizer",
  "authors": [
    {
      "name": "Zorca Orcinus",
      "email": "vs@zorca.org"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "psr/container": "^1.0",
    "latte/latte": "^2.4",
    "symfony/finder": "^3.4",
    "symfony/yaml": "^3.4"
  },
  "require-dev": {
    "10up/wp_mock": "~0.2",
    "antecedent/patchwork": "^2.0",
    "brain/monkey": "^2.3",
    "phpunit/phpunit": "^5.7",
    "lucatume/function-mocker": "~1.3",
    "wp-coding-standards/wpcs": "^2.0",
    "php-coveralls/php-coveralls": "^2.1",
    "wp-cli/wp-cli": "dev-master"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "includes",
      "tests/phpunit/classes",
      "vendor/wp-cli"
    ]
  }
}
